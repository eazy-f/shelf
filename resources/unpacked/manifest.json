{
  "name": "shelf",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "compiled/background/goog/base.js",
      "setup.js",
      "compiled/background/cljs_deps.js",
      "background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_popup": "credentials.html",
    "default_icon": "icons/popup.svg"
  },
  "permissions": [
    "*://localhost/*",
    "https://firebasestorage.googleapis.com/*",
    "storage",
    "bookmarks",
    "nativeMessaging"
  ],
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "shelf@eazy-f"
    }
  }
}
